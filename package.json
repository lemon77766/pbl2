{
  "name": "poem-app",
  "version": "1.0.0",
  "type": "module",
  "description": "诗词赏析应用 - 基于Vue 3和Supabase",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "pinia": "^2.1.6",
    "element-plus": "^2.3.8",
    "@element-plus/icons-vue": "^2.1.0",
    "@supabase/supabase-js": "^2.33.1",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vue-tsc": "^1.6.4",
    "vitest": "^0.32.0",
    "@vue/test-utils": "^2.3.2",
    "@vitest/ui": "^0.32.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "@types/node": "^20.4.2"
  },
  "keywords": ["vue", "typescript", "poem", "supabase", "诗词赏析"],
  "author": "诗词赏析项目组",
  "license": "MIT"
}